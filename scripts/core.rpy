style FH_chapter_style:
    outlines [(absolute(3),"#000",absolute(2),absolute(2))]
    font FH_Nexa_Script
    color '#fff'
    size 90

style FH_subs:
    is text
    font FH_Zekton
    size 60
    outlines [ (absolute(2), "#000", 2, 2), (absolute(2), "#000", 0, 0) ]
    xalign 0.5
    yalign 0.7
    text_align 0.5
    
style FH_subs_dv:
    is FH_subs
    color "#FFAA00"
    
style FH_subs_al:
    is FH_subs
    color "#919191"

style FH_subs_un:
    is FH_subs
    color "#AA64D9"

style FH_subs_vi:
    is FH_subs
    color "#1E7628"

style FH_subs_me:
    is FH_subs
    color "#E1DD7D"
    
image FH_subs = ParameterizedText(style = "FH_subs")
image FH_chapter = ParameterizedText(style = "FH_chapter_style")

label FH_chapter(cgn, bgn, spn, dayn, cgpos, textpos):
    $ fix, fix2 = int(cgpos), float(textpos)
    scene black with dissolve
    pause 1
    scene expression cgn:
        xpos -fix
    show expression AlphaMask(bgn, 'mods/four_horizons/images/misc/alpha_mask.png'):
        blur 10
    show expression spn:
        xpos 1070
    show FH_chapter '{=FH_chapter_style}[dayn]{/FH_chapter_style}':
        xalign fix2
        yalign 0.8
    with dissolve2
    pause 3
    show black with moveinright
    return

label FH_credits(girl):
    $ persistent.FH_ending[girl] = True
    scene bg black
    play music "<from 80>mods/four_horizons/music/Кино - Малыш (голос).mp3" fadein 3 noloop
    if girl == "dv":
        scene bg ext_stage_normal_night
    elif girl == "sl":
        scene bg ext_square_night
    elif girl == "un":
        scene bg ext_boathouse_night
    elif girl == "us":
        scene bg ext_playground_night
    with dissolve
    show expression "mods/four_horizons/images/misc/credits_" + girl + ".png":
        ypos 1.0
        linear 108 ypos -7.0
    $ renpy.pause(108, hard = True)
    stop music fadeout 3
    scene bg black with dissolve2
    pause 1
    $ renpy.block_rollback()
    jump FH_start
